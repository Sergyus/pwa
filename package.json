{
  "homepage": "https://sergyus.github.io/pwa/",
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/server/**/*' -e ts,tsx --exec ts-node -r tsconfig-paths/register --files .",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --transpile-only --files .",
    "build": "yarn clean:build && cross-env NODE_ENV=production webpack --progress --config ./webpack/config.babel.js",
    "build-sw": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.sw.config.js -w",
    "clean:build": "rimraf ./public/assets ./public/loadable-stats.json ./public/service-* ./public/webpack-assets.json",
    "code-gen": "graphql-codegen --config codegen.yml",
    "prettier": "prettier --ignore-path=.prettierignore --write --config .prettierrc.json \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:fix": "eslint --fix src/**/*{.ts,.tsx} && yarn prettier",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:update": "yarn test -u",
    "analyze": "npm run build -- --env addon=bundleanalyze",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --verbose",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "**/*": "prettier -w -u",
    "*.{js,jsx,jx,tsx}": [
      "eslint --fix"
    ]
  },
